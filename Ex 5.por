programa {
	funcao inicio() {
		
		
		
		real salfixo, salfin, cv, vend
		
		
	    escreva ("Qual seu salário fixo: R$ ")
		leia  (salfixo)
	    escreva ("Qual o valor das vendas: R$ ")
	    leia (vend)
	    escreva ("Comissão sobre as vendas: R$ ")
	    cv = vend*0.04
